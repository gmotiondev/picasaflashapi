package{
	import flash.display.DisplayObject;
	import flash.display.MovieClip;
	import flash.events.Event;
	import flash.utils.getDefinitionByName;
		[SWF(width="512", height="526", backgroundColor="0x111111", frameRate="31")]		public class TumblrApp extends MovieClip	{			public function TumblrApp()		{			stop();						addEventListener(Event.ENTER_FRAME, enterFrame_Handler);		}				private function enterFrame_Handler(evt : Event) : void		{			var tPerc : Number = (this.loaderInfo.bytesLoaded / this.loaderInfo.bytesTotal);						this.graphics.clear();			this.graphics.beginFill(0x000000);			this.graphics.drawRect(0, 0, this.stage.stageWidth * tPerc, this.stage.stageHeight);			this.graphics.endFill();						if(framesLoaded == totalFrames)			{				this.removeEventListener(Event.ENTER_FRAME, enterFrame_Handler);								nextFrame();				initialize_Handler();			}		}				private function initialize_Handler() : void		{			var mainClass : Class = Class(getDefinitionByName("MainApplication"));					if(mainClass)			{				var app : Object = new mainClass();				this.addChild(app as DisplayObject);			}		}	}}